// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./data.db"
}

model User {
id Int @id @default(autoincrement())
name String
photo String
email String
hobby Hobby[]
}
model Hobby {
id Int @id @default(autoincrement())
name String
image String
active Boolean
user User @relation(fields: [userId], references: [id])
userId Int
}